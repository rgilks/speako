{
  "name": "speako",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "preview": "vite preview",
    "prepare": "husky",
    "deps:update": "npx npm-check-updates -u",
    "validate": "npx tsx scripts/validate.ts",
    "validate:report": "npx tsx scripts/benchmark-report.ts",
    "prepare:data": "npx tsx scripts/prepare-validation-data.ts",
    "prepare:models": "npx tsx scripts/download-models.ts"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.8.1",
    "@preact/signals": "^1.2.0",
    "compromise": "^14.14.4",
    "preact": "^10.19.0",
    "wavesurfer.js": "^7.12.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@preact/preset-vite": "^2.8.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/preact": "^3.2.4",
    "@types/fast-levenshtein": "^0.0.4",
    "fast-levenshtein": "^3.0.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^1.0.0"
  }
}
